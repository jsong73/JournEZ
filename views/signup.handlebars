<div class="box" id="signup-box">
	<form>
		<span class="text-center"> Sign up </span>
  
<div class="input-container">
<label> First name </label> <br> <input type="firstname" id="first-name"></div>

<div class="input-container">
<label> Last name </label> <br> <input type="lastname" id="last-name"></div>

  
<div class="input-container">
<label> Email </label> <br> <input type="email" id="email"></div>
  
<div class="input-container">
<label> Username </label> <br> <input type="username" id="username">
<br>
  
<div class="input-container">
<label> Password </label> <br> <input type="password" id="password" autocomplete="on"></div>
  
<div class="input-container">
<label> Confirm password </label> <br> <input type="password" id="confirmpass" autocomplete="on"></div>

<button id="signupbtn"> Create new account </button>
</div>

</form>

<button class="loginbtn" id="signinbtn"> Log in </button>


<script src="/assets/signup.js"></script>
